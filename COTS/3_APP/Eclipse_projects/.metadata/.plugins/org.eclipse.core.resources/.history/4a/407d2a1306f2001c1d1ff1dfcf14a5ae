#ifndef	UART_private_H_
#define UART_private_H_


#define FIVE_BITS	0
#define SIX_BITS	1
#define SEVEN_BITS	2
#define EIGHT_BITS	3
#define NINE_BITS	4


#define EVEN_PARITY		0
#define ODD_PARITY		1
#define DISABLED		2



#endif
